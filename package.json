  "bugs": {
    "url": "https://github.com/Pallav2003/proshop/issues"
  },
  "homepage": "https://github.com/Pallav2003/proshop#readme",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "express": "^5.1.0",
    "mongoose": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "nodemon": "^3.1.10"
  }
}
